# Recipe for installing Palo Alto GlobalProtect on macOS

{
  "Name": "PaloAltoGlobalProtect",
  "Identifier": "com.github.autopkg.PaloAltoGlobalProtect",
  "ParentRecipe": "nopkg",
  "MinimumVersion": "0.7.0",

  "Input": {
    "URL": "https://gp1.southperth.wa.gov.au/global-protect/getmsi.esp?platform=mac",
    "FILENAME": "GlobalProtect.pkg"
  },

  "Process": [
    {
      "Name": "move",
      "DestinationPath": "/private/tmp/GlobalProtect.pkg"
    }
  ]
}
